<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Atkinson Hyperlegible Next Regular (replaces Inter) -->
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/inter_regular" />
    <font
        android:fontStyle="italic"
        android:fontWeight="400"
        android:font="@font/inter_italic" />
    
    <!-- Atkinson Hyperlegible Next Medium (replaces Inter) -->
    <font
        android:fontStyle="normal"
        android:fontWeight="500"
        android:font="@font/inter_medium" />
    <font
        android:fontStyle="italic"
        android:fontWeight="500"
        android:font="@font/inter_medium_italic" />
</font-family>